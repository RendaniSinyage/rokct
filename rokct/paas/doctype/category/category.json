{
    "doctype": "DocType",
    "name": "Category",
    "engine": "InnoDB",
    "is_submittable": 0,
    "issingle": 0,
    "istable": 0,
    "module": "Paas",
    "permissions": [
        {
            "create": 1,
            "delete": 1,
            "email": 1,
            "export": 1,
            "print": 1,
            "read": 1,
            "report": 1,
            "role": "System Manager",
            "share": 1,
            "write": 1
        }
    ],
    "sort_field": "modified",
    "sort_order": "DESC",
    "track_changes": 1,
    "fields": [
        {
            "fieldname": "uuid",
            "fieldtype": "Data",
            "label": "UUID",
            "reqd": 1,
            "unique": 1,
            "no_copy": 1
        },
        {
            "fieldname": "slug",
            "fieldtype": "Data",
            "label": "Slug"
        },
        {
            "fieldname": "keywords",
            "fieldtype": "Small Text",
            "label": "Keywords"
        },
        {
            "fieldname": "parent_category",
            "fieldtype": "Link",
            "label": "Parent Category",
            "options": "Category"
        },
        {
            "fieldname": "type",
            "fieldtype": "Select",
            "label": "Type",
            "options": "main\nsub_main\nblog\nbrand\nshop\nreceipt\nmenu\ncareer\nsub_shop"
        },
        {
            "fieldname": "image",
            "fieldtype": "Attach Image",
            "label": "Image"
        },
        {
            "fieldname": "active",
            "fieldtype": "Check",
            "label": "Active",
            "default": "1"
        },
        {
            "fieldname": "status",
            "fieldtype": "Select",
            "label": "Status",
            "options": "published\npending\nunpublished"
        },
        {
            "fieldname": "shop",
            "fieldtype": "Link",
            "label": "Shop",
            "options": "Shop"
        },
        {
            "fieldname": "input",
            "fieldtype": "Int",
            "label": "Input"
        },
        {
            "fieldname": "translations",
            "fieldtype": "Table",
            "label": "Translations",
            "options": "Category Translation"
        }
    ]
}

